<div id='mobile-menu' class='mobile-menu hide'>
  <div class='grid-x'>
    <div class='cell center'>
      <img src="<% if(dirName) { %><%=dirName%><%}%>/img/logos/logo.png" class='top-bar-logo '>
    </div>
    <% for(let i = 0; i < pages.length; i++) { %>
      <div class='cell center mobile-menu-item'>
        <a class='top-bar-link' onclick="mobileMenu()" href="<% if(dirName) { %><%=dirName%><%}%><%= pages[i].link %>"><%= pages[i].title %></a>
      </div>
    <% } %>
  </div>
</div>